{
    "name": "vscode-selection-extended",
    "displayName": "vscode selection extended",
    "description": "Additional selection options like select inverse, select till end of file, select till start of file and more...",
    "version": "0.0.1",
    "publisher": "cg-cnu",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "author": {
        "email": "sreenivas9alapati@gmail.com",
        "name": "Sreenivas Alapati",
        "url": "https://twitter.com/cgcnu"
    },
    "bugs": {
        "email": "sreenivas9alapati@gmail.com"
    },
    "activationEvents": [
        "onCommand:selection.tillSOF",
        "onCommand:selection.tillEOF",
        "onCommand:selection.inverse"
    ],
    "main": "./out/src/selection",
    "contributes": {
        "commands": [
            {
                "command": "selection.tillSOF",
                "title": "select: Till Start of file"
            },
            {
                "command": "selection.tillEOF",
                "title": "select: Till End of file"
            },
            {
                "command": "selection.inverse",
                "title": "select: Inverse"
            }

        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
